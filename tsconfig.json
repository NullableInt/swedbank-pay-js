{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "strictPropertyInitialization": false,
  "compilerOptions": {
    "rootDir": ".",
    "target": "es6",
    "outDir": "build"
  },
  "include": [
    "src/*.ts",
    "src/**/*.ts",
    "test/*.ts",
    "test/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
